{
  "name": "start-tab",
  "version": "1.2.0",
  "description": "Custom new tab.",
  "author": "Jonathan Crocker",
  "license": "ISC",
  "source": "src/index.html",
  "scripts": {
    "cleanparcel": "mkdirp ./.parcel-cache && rimraf ./.parcel-cache",
    "cleandist": "mkdirp ./dist && rimraf ./dist/*",
    "cleanobj": "mkdirp ./obj && rimraf ./obj/* && rimraf ./obj/*",
    "cleannode": "mkdirp ./node_modules && rimraf ./node_modules",
    "clean": "npm run cleanparcel && npm run cleandist && npm run cleanobj && npm run cleannode",
    "prebuild": "mkdirp dist && rimraf dist/* && copyfiles --flat src/manifest.json src/background.js dist",
    "build": "parcel build --no-content-hash --no-source-maps",
    "postbuild": "mkdirp obj && rimraf obj/* && npm-build-zip --source=dist --destination=obj"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.8.3",
    "@types/chrome": "0.0.224",
    "@types/luxon": "3.2.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/bootstrap": "5.2.6",
    "bootstrap": "5.2.3",
    "luxon": "3.3.0",
    "parcel": "2.8.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.5",
    "rimraf": "3.0.2",
    "mkdirp": "2.1.5",
    "copyfiles": "2.4.1",
    "npm-build-zip": "1.0.3",
    "react-icons": "4.8.0"
  }
}
